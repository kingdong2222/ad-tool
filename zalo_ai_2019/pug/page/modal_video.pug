block Modal

.modal.fade(id="modal-video" tabindex="-1" role="dialog")
	.modal-dialog.is-large(role="document")
		.modal-content
			.modal-body.is-non
				.context-video
					video#video1(width="400" controls)
						source(src="https://streaming.interactive.brand.zing.vn/media/videos/Zalo%20AI%20Summit%202018.mp4" type="video/mp4")
			a.func-close-modal(type="button" data-dismiss="modal" aria-label="Close")
				i.icn.icn-small-remove
